<div class="row">
  <div class="col-md-12 actions">
    <div class="page-header">
      <%= link_to '<i class="fa fa-chevron-left"></i> Volver'.html_safe, me_vehicles_path, :class => 'btn btn-default btn-lg pull-right' %>
      <h2><%= @page_title %></h2>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= form_for @vehicle, url: me_vehicle_path(@vehicle), html: {class: 'form-horizontal'} do |f| %>
      <%= render 'form', f: f %>
    <% end %>
  </div>
</div>